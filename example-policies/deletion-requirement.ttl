@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/policy001>
  a odrl:Policy ;
  odrl:profile <http://www.w3.org/ns/odrl.jsonld> ;
  odrl:assigner <https://www.example.com/assigner> ;
  odrl:assignee <https://www.example.com/assignee> ;
  odrl:obligation [
    a odrl:Obligation ;
    odrl:target <https://www.example.com/data1.txt> ;
    odrl:action odrl:delete ;
    odrl:constraint [
      a odrl:Constraint ;
      odrl:leftOperand odrl:dateTime ;
      odrl:operator odrl:lt ;
      odrl:rightOperand "2023-06-23T00:00:00Z"^^xsd:dateTime
    ]
  ] .
